const args = OS.GetArguments();
function PrintEnvironment()
{
	foreach(env in OS.Environment.GetVariables())
	{
		OS.WriteLine(env.Key + " : " + env.Value);
	}
}


if(args.Length == 1)
{
	PrintEnvironment();
}
else
{
	const envName = args[1];
	if(OS.Environment.Has(envName))
	{
		OS.WriteLine(envName + " : " + OS.Environment.Get(envName));
	}
	else
	{
		OS.WriteLine($"Environment Variable '{envName}' is not set");
	}
}