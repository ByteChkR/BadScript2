
const args = OS.GetArguments();
if(args.Length < 2)
{
	OS.WriteLine("Expected at least one Argument");
}
else
{
	foreach(file in args.Skip(1))
	{
		if(OS.FileSystem.Directory.Exists(file))
		{
			OS.FileSystem.Directory.Delete(file, true);
		}
		else if(OS.FileSystem.File.Exists(file))
		{
			OS.FileSystem.File.Delete(file);
		}
	}
}