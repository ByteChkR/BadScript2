
let Assert = NUnit.Assert;

function True() => true;
function Add(a, b) => a + b;
function Test_Functions()
{
	Assert.IsTrue(True(), "True() == true");
	Assert.AreEqual(Add(5, 5), 10, "Add(5, 5) == 10");

	let func = function(a, b) => a - b;
	Assert.Zero(func(5, 5), "func(5, 5) == 0");
}

NUnit.AddTest(Test_Functions, "Function Tests");