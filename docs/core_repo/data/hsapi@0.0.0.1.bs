let Package;function(){Package = Runtime.Import("Package");}(); let HighscoreApi;function(){HighscoreApi = Package.Import("HighscoreApi");}(); const args = Runtime.GetArguments(); function Main() { if(args.Length < 2) { Console.WriteLine("Invalid Argument Count."); Console.WriteLine("Expected Usage: bs hsapi <apiKey> <prefix>, ..."); return; } const apiKey = args[0]; const prefixes = args.Skip(1).ToArray(); const api = new HighscoreApi(prefixes, apiKey); Console.WriteLine("Press enter to exit"); api.Start(); } Main();